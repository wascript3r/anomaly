{
    "log": {
        "showTimestamp": true
    },

    "database": {
        "postgres": {
            "dsn": "postgres://postgres:secret@db/anomaly?sslmode=disable",
            "queryTimeout": "10s"
        }
    },

    "anomaly": {
        "threshold": 85
    },

    "request": {
        "dateTimeFormat": "2006-01-02 15:04:05"
    },

    "http": {
        "port": "80",
        "enablePprof": true
    }
}
